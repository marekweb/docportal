	src: url('/webfonts/CalibreWeb-Bold.eot') %>#iefix') format('embedded-opentype'),
		url('/webfonts/CalibreWeb-Bold.woff2') %>') format('woff2'),
		url(/webfonts/CalibreWeb-Bold.woff') %>') format('woff');
	font-weight: bold;
	font-style: normal;
}
