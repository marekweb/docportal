@font-face {
	font-family: 'Calibre Web';
	src: url('<%= asset_path('assets/fonts/CalibreWeb-Light.eot') %>');
	src: url('<%= asset_path('assets/fonts/CalibreWeb-Light.eot') %>#iefix') format('embedded-opentype'),
		url('<%= asset_path('assets/fonts/CalibreWeb-Light.woff2') %>') format('woff2'),
		url('<%= asset_path('assets/fonts/CalibreWeb-Light.woff') %>') format('woff');
	font-weight: normal;
	font-style: normal;
}

@font-face {
	font-family: 'Calibre Web';
	src: url('<%= asset_path('fonts/CalibreWeb-Bold.eot') %>');
	src: url('<%= asset_path('fonts/CalibreWeb-Bold.eot') %>#iefix') format('embedded-opentype'),
		url('<%= asset_path('fonts/CalibreWeb-Bold.woff2') %>') format('woff2'),
		url('<%= asset_path('fonts/CalibreWeb-Bold.woff') %>') format('woff');
	font-weight: bold;
	font-style: normal;
}
